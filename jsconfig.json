{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "module": "es2020",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@components": ["front/components"],
      "@conatiners/*": ["front/containers/*"],
      "@constants/*": ["front/constants/*"],
      "@config/*": ["front/config/*"],
      "@assets/*": ["front/assets/*"],
      "@hooks/*": ["front/hooks/*"],
      "@styles/*": ["front/styles/*"],
      "@utils/*": ["front/utils/*"],
      "@pages/*": ["front/pages/*"],
      "@store/*": ["front/store/*"],
      "@reducers/*": ["front/reducers/*"],
      "@sagas/*": ["front/sagas/*"]
    }
  }
}
